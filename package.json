{
  "name": "react-cxs",
  "version": "1.0.0-beta.1",
  "description": "Alternative React.createElement function which allows style objects to be passed to the className prop to generate CSS using cxs",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "mkdir -p dist && babel src --out-dir dist",
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "gh-pages": "gh-pages -d demo",
    "test": "ava test -v"
  },
  "keywords": [
    "react",
    "jsx",
    "css",
    "css-in-js"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "cxs": "^1.0.0-beta.9"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "color": "^0.11.3",
    "gh-pages": "^0.11.0",
    "highlight-loader": "^0.7.2",
    "html-loader": "^0.4.3",
    "jsdom": "^9.4.1",
    "jsdom-global": "^2.0.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "react": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-dom": "^15.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
